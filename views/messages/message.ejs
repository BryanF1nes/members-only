<% if (messages.length === 0) { %>
    <div>
        <h3 class="text-lg text-gray-900">Be the first to send a message!</h3>
    </div>
<% } else { %>
    <div id="messageDiv"></div>
    <script>
        let messages = <%- JSON.stringify(messages) %>;
    </script>
    <script src="/js/message.js"></script>
<% } %>